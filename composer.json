{
    "name": "siberfx/shoppingcart",
    "description": "Laravel9 Shopping Cart",
    "keywords": ["laravel 9", "shoppingcart", "cart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Selim Gormus",
            "email": "info@siberfx.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/support": "^9.39",
        "illuminate/session": "^9.39",
        "illuminate/events": "^9.39"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.11"
    },
    "autoload": {
        "psr-4": {
            "Siberfx\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siberfx\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Siberfx\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Siberfx\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
